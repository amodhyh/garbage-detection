    "def denoise(image):\n",
    "    h, w = image.shape[:2]\n",
    "    # Kernel size: 1/50th of the smallest dimension, rounded to nearest odd integer\n",
    "    ksize = max(3, min(15, (min(h, w) // 50) | 1))\n",
    "    blurred_image = cv2.GaussianBlur(image, (ksize, ksize), 0)\n",
    "    return blurred_image\n"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "90841a104afbbfb0",
   "metadata": {},
   "source": [
    "#### Sharpen"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 32,
   "id": "76cc136dd9a442fb",
   "metadata": {
    "ExecuteTime": {
     "end_time": "2025-09-23T13:52:40.653835Z",
     "start_time": "2025-09-23T13:52:40.647303Z"
    }
   },
   "outputs": [],
   "source": [
    "def sharpen(image):\n",
    "    h, w = image.shape[:2]\n",
    "    # Sharpening strength: scale with image size, but keep reasonable bounds\n",
    "    alpha = min(2.0, max(1.0, min(h, w) / 128))\n",
    "    # Sharpening kernel\n",
    "    kernel = np.array([[0, -1, 0],\n",
    "                       [-1, 4 * alpha + 1, -1],\n",
    "                       [0, -1, 0]])\n",
    "    kernel = kernel / (4 * alpha)\n",
    "    sharpened = cv2.filter2D(image, -1, kernel)\n",
    "    return sharpened\n"
   ]
  },
